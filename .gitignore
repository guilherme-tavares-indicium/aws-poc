# Ignore .terraform directories
**/.terraform/*

# Ignore .tfstate files
*.tfstate
*.tfstate.*

# Ignore .tfvars files that are automatically generated
*.auto.tfvars
*.auto.tfvars.json
*.auto.tfvars.tf

# Ignore crash log files
crash.log

# Ignore any backup files created by text editors or IDEs
*~
*.bak
*.swp
*.backup
*.save

# Ignore the .idea directory created by IntelliJ IDEA
.idea/

# Ignore the .vscode directory created by Visual Studio Code
.vscode/

# Ignore .terraform.lock.hcl files
*.lock.hcl

# Ignore .terraformrc files
.terraformrc

# Ignore .terraform-version files
.terraform-version

# Ignore Python virtual environment folders
venv/
env/

# Ignore compiled Python files
*.pyc
*.pyo
__pycache__/

# Ignore log files
*.log

# Ignore data files
data/

# Ignore etl job output files
output/

# Ignore credentials file
*.creds

# Ignore etl scripts config files
*.conf

# Ignore etl scripts dependencies
*.egg-info/
dist/
build/

# Ignore any files in the etl_scripts folder that don't need to be versioned
etl_scripts/__pycache__/
etl_scripts/*.pyc
etl_scripts/*.pyo
etl_scripts/*.log
etl_scripts/*.txt
etl_scripts/*.csv
etl_scripts/.env
!etl_scripts/iris.csv
